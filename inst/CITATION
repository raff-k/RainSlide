citEntry(entry = "Manual",
         title = "RainSlide: Rainfall-induced landslide analysis tools",
         author = personList(as.person("Raphael Knevels")),
         year = "2020",
         note = "R package version 0.0.0-9000",
         url = "https://github.com/raff-k/RainSlide",
         header = "To cite the 'RainSlide' package use:",
         textVersion =
           paste("Knevels, R. 2020",
                 "RainSlide: Rainfall-induced landslide analysis tools.",
                 "R package version 0.0.0-9001.",
                 "https://github.com/raff-k/RainSlide")
)

